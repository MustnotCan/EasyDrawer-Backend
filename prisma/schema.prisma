generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model book{
  id String @id @db.Uuid @default(uuid())
  title String @db.VarChar(255) 
  tags tag[] 
  path String @db.VarChar(500) @unique()
  description String? @db.Text()
  lastOpened DateTime? 
}
model tag{
id String @id @db.Uuid @default(uuid())
name String @unique()
book book[] 
}

