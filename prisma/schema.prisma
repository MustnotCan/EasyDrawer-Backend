generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model book{
  id Int @id  @default(autoincrement())
  title String @db.VarChar(255) 
  thumbnailPath String  
  tags tag[] 
  path String @db.VarChar(500) @unique()
  description String? @db.Text()
  lastOpened DateTime? 
}
model tag{
id Int @id @default(autoincrement())
name String @unique()
book book[] 
}
